isbn = {1364-5072 (Print)$\backslash$n1364-5072 (Linking)},
issn = {13645072},
journal = {Journal of Applied Microbiology},
keywords = {16S rDNA,Faecal DNA,Human GI tract microbiota,Irritable bowel syndrome,Real-time PCR},
number = {6},
pages = {1166--1177},
pmid = {15546407},
title = {{Development of an extensive set of 16S rDNA-targeted primers for quantification of pathogenic and indigenous bacteria in faecal samples by real-time PCR}},
