  eprinttype   = {pmid},
  file         = {/Users/byronsmith/Zotero/storage/4CHRNVX5/Yamamoto,
    Otsuki - 2006 - Effect of inhibition of α-glucosidase on
    age-related glucose intolerance and pancreatic atrophy in rats.pdf},
  isbn         = {0026-0495},
  number       = 4,
}
