keywords = {-amylase,-glucosidase,acarbose,acarbose is a pseudotetrasaccharide,acarviosine,bose,consisting of an,cyclomaltodextrin glucanosyltransferase,enzyme inhibitors,glucose,isoacar-},
number = {2},
pages = {277--283},
pmid = {10545215},
title = {{Comparative study of the inhibition of alpha-glucosidase, alpha-amylase, and cyclomaltodextrin glucanosyltransferase by acarbose, isoacarbose, and acarviosine-glucose.}},
