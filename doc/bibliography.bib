journal = {Cellular and Molecular Life Sciences},
keywords = {Bacteroides thetaiotaomicron,Gut microbiota,Starch,Starch utilization system,Sus,bacteroides thetaiotaomicron,gut microbiota,starch,starch utilization system,sus},
number = {14},
