keywords = {16S/genetics,Biodiversity,Diabetes Mellitus,Female,Gastrointestinal Microbiome/*drug effects/genetics,Humans,Hypoglycemic Agents/pharmacology/therapeutic use,Male,Metagenome/drug effects/physiology,Metformin/*pharmacology/therapeutic use,RNA,Ribosomal,Type 2/drug therapy/*microbiolo},
month = {dec},
