  eprinttype   = {pmid},
  file         = {/Users/byronsmith/Zotero/storage/4PV3WNZN/Nguyen et
    al. - 2015 - How informative is the mouse for human gut microbiota
    research.pdf;/Users/byronsmith/Zotero/storage/XZ2RQXBK/Nguyen et
    al. - 2015 - How informative is the mouse for human gut microbiota
    research(2).pdf},
  isbn         = {1754-8411},
