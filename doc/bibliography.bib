  eprinttype   = {pmid},
  file         = {/Users/byronsmith/Zotero/storage/7WSTJPU8/Kozich et
    al. - 2013 - Development of a dual-index sequencing strategy and
    curation pipeline for analyzing amplicon sequence data on the.pdf},
