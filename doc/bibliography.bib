@article{OHara2006,
  title        = {The gut flora as a forgotten organ},
  author       = {O'Hara, Ann M. and Shanahan, Fergus},
  date         = 2006,
  journaltitle = {EMBO Reports},
  volume       = 7,
  pages        = {688--693},
  issn         = {1469221X},
  doi          = {10.1038/sj.embor.7400731},
  abstract     = {The intestinal microflora is a positive health asset
    that crucially influences the normal structural and functional
    development of the mucosal immune system. Mucosal immune responses
    to resident intestinal microflora require precise control and an
    immunosensory capacity for distinguishing commensal from
    pathogenic bacteria. In genetically susceptible individuals, some
    components of the flora can become a liability and contribute to
    the pathogenesis of various intestinal disorders, including
    inflammatory bowel diseases. It follows that manipulation of the
    flora to enhance the beneficial components represents a promising
    therapeutic strategy. The flora has a collective metabolic
    activity equal to a virtual organ within an organ, and the
    mechanisms underlying the conditioning influence of the bacteria
    on mucosal homeostasis and immune responses are beginning to be
    unravelled. An improved understanding of this hidden organ will
    reveal secrets that are relevant to human health and to several
    infectious, inflammatory and neoplastic disease processes.},
  eprint       = 16819463,
