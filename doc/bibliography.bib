journal = {The Journal of Nutrition},
number = {2},
pages = {213--220},
pmid = {22223580},
title = {{Prebiotic Fiber Increases Hepatic Acetyl CoA Carboxylase Phosphorylation and Suppresses Glucose-Dependent Insulinotropic Polypeptide Secretion More Effectively When Used with Metformin in Obese Rats}},
