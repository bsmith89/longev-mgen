journal = {Perspectives on Psychological Science},
keywords = {design calculation,exaggeration ratio,power analysis,replication crisis,statistical significance,type m error,type s error},
number = {6},
pages = {641--651},
pmid = {26186114},
title = {{Beyond Power Calculations: Assessing Type S (Sign) and Type M (Magnitude) Errors}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26186114{\%}5Cnhttp://pps.sagepub.com/lookup/doi/10.1177/1745691614551642{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/?term=10.1177/1745691614551642{\%}5Cnhttp://pps.sagepub.com/lookup/doi/10.1177/1745691614551642},
