@article{Jernberg2010,
  title        = {Long-term impacts of antibiotic exposure on the
    human intestinal microbiota},
  author       = {Jernberg, Cecilia and Löfmark, Sonja and Edlund,
    Charlotta and Jansson, Janet K.},
  date         = 2010,
  journaltitle = {Microbiology},
  volume       = 156,
  pages        = {3216--3223},
  issn         = 13500872,
  doi          = {10.1099/mic.0.040618-0},
  abstract     = {Although it is known that antibiotics have
    short-term impacts on the human microbiome, recent evidence
    demonstrates that the impacts of some antibiotics remain for
    extended periods of time. In addition, antibiotic-resistant
    strains can persist in the human host environment in the absence
    of selective pressure. Both molecular- and cultivation-based
    approaches have revealed ecological disturbances in the microbiota
    after antibiotic administration, in particular for specific
    members of the bacterial community that are susceptible or
    alternatively resistant to the antibiotic in question. A
    disturbing consequence of antibiotic treatment has been the
    long-term persistence of antibiotic resistance genes, for example
    in the human gut. These data warrant use of prudence in the
    administration of antibiotics that could aggravate the growing
    battle with emerging antibiotic-resistant pathogenic strains.},
  eprint       = 20705661,
  eprinttype   = {pmid},
  file         = {/Users/byronsmith/Zotero/storage/WQ83UCGB/Jernberg
    et al. - 2010 - Long-term impacts of antibiotic exposure on the
    human intestinal microbiota.pdf},
  isbn         = {1465-2080 (Electronic)\textbackslash n1350-0872
    (Linking)},
  number       = 11,
}
@article{Jiang2017,
  title        = {Discrete false-discovery rate improves
    identification of differentially abundant microbes},
  author       = {Jiang, Lingjing and Amir, Amnon and Morton, James T.
    and Heller, Ruth and Arias-Castro, Ery and Knight, Rob},
  date         = 2017,
