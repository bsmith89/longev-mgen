issn = {1759-5029},
journal = {Nature Reviews Endocrinology},
number = {10},
pages = {577--591},
pmid = {26260141},
publisher = {Nature Publishing Group},
title = {{Short-chain fatty acids in control of body weight and insulin sensitivity}},
