journal = {Proceedings of the National Academy of Sciences},
number = {20},
pages = {6955--6959},
pmid = {2413450},
title = {{Rapid determination of 16S ribosomal RNA sequences for phylogenetic analyses.}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.82.20.6955},
volume = {82},
year = {1985}
}
