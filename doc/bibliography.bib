file = {:Users/bjsmith/Documents/Literature/2007/Wang et al/2007{\_}Wang et al(2).pdf:pdf;:Users/bjsmith/Documents/Literature/2007/Wang et al/2007{\_}Wang et al.pdf:pdf},
isbn = {0099-2240 (Print)$\backslash$r0099-2240 (Linking)},
issn = {0099-2240},
journal = {Applied and Environmental Microbiology},
keywords = {16S,16S: genetics,Algorithms,Bacteria,Bacteria: classification,Bacteria: genetics,Bayes Theorem,Classification,Classification: methods,Databases,Nucleic Acid,Phylogeny,RNA,Ribosomal,Ribosomal: genetics},
month = {aug},
